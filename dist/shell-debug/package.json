{
  "name": "@author.io/shell-debug",
  "version": "1.8.18",
  "description": "A micro-framework for creating CLI-like experiences. This supports Node.js and browsers. (Debug Mappings)",
  "module": "./index.js",
  "exports": {
    "import": "./index.js",
    "default": "./index.js"
  },
  "browser": "./index.js",
  "keywords": [
    "shell",
    "debug",
    "sourcemap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/author/shell"
  },
  "author": {
    "name": "Corey Butler",
    "url": "http://coreybutler.com"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "*.js"
  ],
  "engines": {
    "node": ">=13.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "_*",
      "_**/*",
      ".**/*",
      "node_modules",
      "karma.conf.js",
      "karma.conf.cjs",
      "build.js"
    ],
    "globals": [
      "window",
      "global",
      "globalThis"
    ]
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  }
}